{
  "expo": {
    "name": "Starbuck Mockup App",
    "slug": "starbuck-mockup-app",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "app to mockup starbuck UI with login features",
    "android": {
      "package": "com.kenny.ic.ho.starbuckmockupapp"
    }
  }
}
